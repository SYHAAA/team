package com.seecen.domain;

import com.fasterxml.jackson.annotation.JsonFormat;

import java.math.BigDecimal;
import java.util.Date;

public class ReviewerInformation {
    private Integer plaId;

    private Integer rlyId;

    private String rlyContent;

    private Integer uplevelId;

    @JsonFormat(pattern="yyyy-MM-dd HH:mm:ss",timezone = "GMT+8")
    private Date repTime;

    private Integer valid;

    public Integer getPlaId() {
        return plaId;
    }

    public void setPlaId(Integer plaId) {
        this.plaId = plaId;
    }

    public Integer getRlyId() {
        return rlyId;
    }

    public void setRlyId(Integer rlyId) {
        this.rlyId = rlyId;
    }

    public String getRlyContent() {
        return rlyContent;
    }

    public void setRlyContent(String rlyContent) {
        this.rlyContent = rlyContent;
    }

    public Integer getUplevelId() {
        return uplevelId;
    }

    public void setUplevelId(Integer uplevelId) {
        this.uplevelId = uplevelId;
    }

    public Date getRepTime() {
        return repTime;
    }

    public void setRepTime(Date repTime) {
        this.repTime = repTime;
    }

    public Integer getValid() {
        return valid;
    }

    public void setValid(Integer valid) {
        this.valid = valid;
    }
}